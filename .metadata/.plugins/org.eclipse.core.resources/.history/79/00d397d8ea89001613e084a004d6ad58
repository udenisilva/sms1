package lk.nbro.lsmis.model;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.EnumType;
import javax.persistence.Enumerated;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

import lk.nbro.lsmis.constant.GeneralEnumConstants.ApplicationType;
import lk.nbro.lsmis.constant.OLSIMSEnumConstant.UseOfLandType;

@Entity(name = "useOfLand")
@Table(name = "use_of_land")
public class UseOfLand implements Serializable{

	private static final long serialVersionUID = 1L;
	
	@Id
    @GeneratedValue(strategy = GenerationType.AUTO)
    @Column(name = "use_of_land_id", nullable = false)
	private long useOfLandId;
	
	private String description;
	
	@Enumerated(EnumType.STRING)
	private UseOfLandType useOfLandType;
	
	@Enumerated
	private ApplicationType applicationType;

	public long getUseOfLandId() {
		return useOfLandId;
	}

	public void setUseOfLandId(long useOfLandId) {
		this.useOfLandId = useOfLandId;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public ApplicationType getApplicationType() {
		return applicationType;
	}

	public void setApplicationType(ApplicationType applicationType) {
		this.applicationType = applicationType;
	}

	public UseOfLandType getUseOfLandType() {
		return useOfLandType;
	}

	public void setUseOfLandType(UseOfLandType useOfLandType) {
		this.useOfLandType = useOfLandType;
	}
	
}
